{
  "name": "express-joi-openapi",
  "version": "0.2.0",
  "description": "Express Joi OpenAPI",
  "keywords": [
    "express",
    "joi",
    "oas",
    "openapi",
    "swagger"
  ],
  "homepage": "https://github.com/MunifTanjim/express-joi-openapi#readme",
  "bugs": {
    "url": "https://github.com/MunifTanjim/express-joi-openapi/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Munif Tanjim",
    "url": "https://muniftanjim.dev"
  },
  "files": [
    "lib",
    "yarn.lock"
  ],
  "source": "src/index.ts",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/MunifTanjim/express-joi-openapi"
  },
  "scripts": {
    "prebuild": "rimraf lib/*",
    "build": "microbundle --format cjs,es --target node",
    "changelog": "conventional-changelog --config .changelogrc.js -i CHANGELOG.md -s -a",
    "postchangelog": "git add CHANGELOG.md",
    "cz": "git-cz",
    "dev": "microbundle watch --format cjs,es --target node",
    "prepack": "yarn run build",
    "release": "./scripts/release.sh",
    "test": "jest",
    "version": "yarn run changelog"
  },
  "dependencies": {
    "openapi3-ts": "^1.4.0",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/prompt-cli": "^9.1.1",
    "@hapi/joi": "^17.1.1",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "@types/jest": "^26.0.5",
    "@types/lodash": "^4.14.158",
    "@types/ramda": "^0.27.14",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-conventionalcommits": "^4.3.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "microbundle": "^0.12.3",
    "path-to-regexp": "^6.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "@hapi/joi": "^17",
    "express": "^4"
  }
}
